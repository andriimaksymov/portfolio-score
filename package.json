{
  "name": "portfolio-analyzer",
  "version": "0.1.0",
  "private": true,
  "description": "A full-stack application for analyzing developer portfolios with GitHub integration",
  "scripts": {
    "dev": "pnpm --parallel --filter frontend --filter backend dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend start:dev",
    "build": "pnpm --filter frontend build && pnpm --filter backend build",
    "lint": "pnpm --recursive lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "pnpm --recursive type-check"
  },
  "keywords": [
    "portfolio",
    "analyzer",
    "github",
    "developer-tools",
    "react",
    "nestjs",
    "typescript"
  ],
  "author": "Andrii Maksymov",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
